		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class ClientAppPrinterRegistration extends FormRun
{
    SysListPanelRelationTableCallback sysListPanel;

}
]]></Source>
			</Method>
			<Method>
				<Name>availablePrinters</Name>
				<Source><![CDATA[
    container availablePrinters()
    {
        container             availablePrinters, data;
        SysCorpNetPrinters    printers;
        DocumentRoutingPrinterAppAssociation xAppAssociation;

        while select printers   
            notexists join xAppAssociation
            where xAppAssociation.Printer == printers.RecId
                && xAppAssociation.clientApp == DocumentRoutingClientApp.RecId
        {
            data = [printers.recId, printers.PrinterName, printers.PrinterPath];
            AvailablePrinters += [data];
        }

        return AvailablePrinters;
    }

]]></Source>
			</Method>
			<Method>
				<Name>registeredPrinters</Name>
				<Source><![CDATA[
    container registeredPrinters()
    {
        container              selectedPrinters, data;
        SysCorpNetPrinters     printers;
        DocumentRoutingPrinterAppAssociation xAppAssociation;

        while select printers
            exists join xAppAssociation
            where xAppAssociation.Printer == printers.RecId
                && xAppAssociation.ClientApp == DocumentRoutingClientApp.RecId
        {
            data = [printers.recId, printers.PrinterName, printers.PrinterPath];
            selectedPrinters += [data];
        }

        return SelectedPrinters;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        container columns;
        #ResAppl
        columns = [0,0];
        sysListPanel = SysListPanelRelationTableCallback::newForm(
            element,
            element.controlId(
            formControlStr(ClientAppPrinterRegistration, Printers)),
            "Registered Printers",
            "Available Printers",
            #IMagePrinter,
        tableNum(DocumentRoutingPrinterAppAssociation),
            fieldNum(DocumentRoutingPrinterAppAssociation, Printer),
            fieldNum(DocumentRoutingPrinterAppAssociation, ClientApp),
            tableNum(SysCorpNetPrinters),
            fieldNum(SysCorpNetPrinters, RecId),
            columns,
            0,
            '',
            '',
            identifierStr(registeredPrinters),
            identifierStr(availablePrinters));
        super();
        sysListPanel.init();

    }

]]></Source>
			</Method>
			<Method>
				<Name>refillListPanel</Name>
				<Source><![CDATA[
    public void refillListPanel(DocumentRoutingClientApp _clientApp)
    {
        sysListPanel.parmRelationRangeValue(int642Str(_clientApp.recId));
        sysListPanel.parmRelationRangeRecId(_clientApp.recid);
        sysListPanel.fill();    
    }

]]></Source>
